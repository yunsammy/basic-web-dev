{
  "manifest_version": 3,
  "name": "Traceroute Visualization",
  "version": "0.1.0",
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "content_security_policy": {
    "sandbox": "sandbox allow-scripts; script-src-elem https://maps.googleapis.com"
  },
  "sandbox": {
    "pages": ["googlemap.html"]
  }
}
